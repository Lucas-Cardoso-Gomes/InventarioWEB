@model Web.Models.Manutencao

@{
    ViewData["Title"] = "Excluir Manutenção";
}

<h1>@ViewData["Title"]</h1>

<h3>Você tem certeza que deseja excluir este registro?</h3>
<div>
    <h4>Manutenção</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ComputadorMAC)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ComputadorMAC)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataManutencaoHardware)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataManutencaoHardware)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataManutencaoSoftware)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataManutencaoSoftware)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ManutencaoExterna)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ManutencaoExterna)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Data)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Data)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Historico)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Historico)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
